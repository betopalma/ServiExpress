{
  "name": "backend",
  "version": "1.0.0",
  "description": "BackEnd de prueba",
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "test": "jest routes --verbose --detectOpenHandles",
    "test:watch": "npm run test -- --watchAll",
    "dev": "nodemon index.js && npm run format",
    "start": "node index.js",
    "format": "eslint --fix --ext .js,.jsx ."
  },
  "engines": {
    "node": "14.x"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.0",
    "browserslist": "^4.20.4",
    "connect-ensure-login": "^0.1.1",
    "cookie-parser": "^1.4.6",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-session": "^1.17.3",
    "googleapis": "^103.0.0",
    "mercadopago": "^1.5.13",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.7.5",
    "passport": "0.5.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-google-oidc": "^0.1.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.3",
    "sequelize": "^6.20.1",
    "stripe": "^9.8.0"
  },
  "devDependencies": {
    "jest": "^28.1.0",
    "nodemon": "^2.0.16",
    "standard": "^17.0.0",
    "supertest": "^6.2.3"
  },
  "eslintConfig": {
    "extends": "./node_modules/standard/eslintrc.json",
    "rules": {
      "indent": [
        "error",
        2
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ],
      "no-undef": "off",
      "space-before-function-paren": "off",
      "camelcase": "off"
    }
  },
  "pre-commit": [
    "npm run format"
  ]
}
